{
    "name": "apcl-mode",
    "displayName": "apcl mode",
    "description": "Syntax highlighting for apcl",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "apcl",
                "aliases": [
                    "apcl",
                    "apcl"
                ],
                "extensions": [
                    ".apcl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "apcl",
                "scopeName": "source.apcl",
                "path": "./syntaxes/apcl.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    }
}
